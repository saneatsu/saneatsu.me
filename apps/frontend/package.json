{
	"name": "@saneatsu/frontend",
	"version": "0.1.0",
	"private": true,
	"description": "Next.js frontend application",
	"scripts": {
		"build": "next build",
		"build:cloudflare": "npx @opennextjs/cloudflare build",
		"dev": "next dev -p 3333",
		"start": "next start -p 3333",
		"deploy": "wrangler deploy",
		"deploy:dev": "wrangler deploy --env development",
		"deploy:preview": "wrangler deploy --env preview",
		"deploy:prod": "wrangler deploy --env production",
		"preview": "wrangler dev",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:middleware": "vitest run --config vitest.config.middleware.mts",
		"e2e": "playwright test",
		"e2e:ui": "playwright test --ui",
		"storybook": "storybook dev -p 6016",
		"build-storybook": "storybook build",
		"test-storybook": "test-storybook --url http://127.0.0.1:6016",
		"test-storybook:ci": "concurrently -k -s first \"storybook dev -p 6016 --ci\" \"wait-on http://127.0.0.1:6016 && test-storybook --url http://127.0.0.1:6016\"",
		"test-storybook:smart": "./test-storybook-smart.sh"
	},
	"dependencies": {
