{
	"name": "saneatsu",
	"version": "0.1.0",
	"private": true,
	"description": "My portfolio",
	"type": "module",
	"packageManager": "pnpm@9.0.0",
	"scripts": {
		"build": "turbo build",
		"build:cloudflare": "turbo build:cloudflare",
		"dev": "concurrently -n Web,Backend -c cyan,green \"pnpm --filter @saneatsu/web dev\" \"pnpm --filter @saneatsu/backend dev\"",
		"dev:turbo": "turbo dev",
		"dev:worker": "turbo dev:worker",
		"deploy": "turbo deploy",
		"deploy:dev": "turbo deploy:dev",
		"deploy:preview": "turbo deploy:preview",
		"deploy:prod": "turbo deploy:prod",
		"preview": "turbo preview",
		"setup:env": "./scripts/setup-cloudflare-env.sh development",
		"setup:env:dev": "./scripts/setup-cloudflare-env.sh development",
		"setup:env:preview": "./scripts/setup-cloudflare-env.sh preview",
		"setup:env:prod": "./scripts/setup-cloudflare-env.sh production",
		"lint": "turbo lint",
		"test": "turbo test",
		"clean": "turbo clean",
		"type-check": "turbo run type-check",
		"check": "biome check .",
		"prepare": "lefthook install",
		"seed": "cd packages/db && pnpm db:seed"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@types/node": "^24.0.15",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"concurrently": "^9.1.2",
		"lefthook": "^1.12.2",
		"turbo": "^2.5.5",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18.0.0",
		"pnpm": ">=8.0.0"
	},
	"dependencies": {
		"wrangler": "^4.26.0"
	}
}
